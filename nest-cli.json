{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
      "include": "**/templates/*",
      "watchAssets": true,
      "outDir": "dist/"
      },
      {
        "include": "**/templates/image/*",
        "watchAssets": true,
        "outDir": "dist/"
      }
    ]
      
  }
}
